<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PushTitle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/titleColor</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="PushMessage" parent="android:Widget.TextView">
        <item name="android:textColor">@color/messageColor</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="PushMessageMultiLine" parent="android:Widget.TextView">
        <item name="android:textColor">@color/messageColor</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="MetaData" parent="android:Widget.TextView">
        <item name="android:textColor">@color/metaColor</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="Dialog" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@null</item>
    </style>
</resources>